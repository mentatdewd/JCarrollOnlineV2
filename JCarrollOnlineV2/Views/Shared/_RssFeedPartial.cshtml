@using JCarrollOnlineV2.ViewModels.Rss
@model RssFeedViewModel

<section>
    <row>
        <img src="~/Content/images/feed-icon-28x28.png" class="rsslogo"/>
        <h3> Mariners News </h3>
    </row>
</section>
<ul class="rss_ul">
    @foreach (RssFeedItemViewModel rssItemViewModel in Model.RssFeedItems.Take(10))
    {
        @Html.Partial("_RssFeedItemPartial", rssItemViewModel)
    }
</ul>