@using JCarrollOnlineV2.ViewModels
@model HomeViewModel

<div class="text-center">
    <h1>Welcome to JCarrollOnline</h1>
    <hr width="35%" />
</div>
<div>
    <p class="text-center">@Html.ActionLink("Sign up now!", "Register", "Account", null, new { @class = "btn btn-large btn-primary", @id = "registerLink" })</p>
</div>

<div class="image_span">
    <img class="image_span" src="~/Content/images/doubleblueline.gif" />
</div>

@Html.Partial("_LatestForumThreadsPartial", Model.LatestForumThreadsViewModel);
<div class="image_span">
    <img class="image_span" src="~/Content/images/doubleblueline.gif" style="margin-left:auto; margin-right:auto"/>
</div>
<div class="text-center"><h2>Blog</h2></div>
<section class="blogFeed">
    @Html.Partial("_BlogFeedPartial", Model.BlogFeed)
</section>
