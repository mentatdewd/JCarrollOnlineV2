@using JCarrollOnlineV2.ViewModels
@using JCarrollOnlineV2.Entities

@model MicropostFeedViewModel

@if (Model.MicropostFeedItems != null)
{
    @Html.Raw("<ul>")
    foreach (MicropostFeedItemViewModel micropost in Model.MicropostFeedItems.OrderByDescending(i => i.CreatedAt))
    {
        @Html.Partial("_MicropostFeedItemPartial", micropost)
    }
    @Html.Raw("</ul>")
}
