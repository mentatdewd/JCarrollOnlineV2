@using JCarrollOnlineV2.ViewModels.Users
@model UserDetailViewModel

@{
    if (Model.UserStatsViewModel.UserFollowers.Users.ToList().Exists(x => x.User.Id == Model.UserInfoViewModel.UserId) == true)
    {
        @Html.Partial("_UnfollowPartial", Model.UserInfoViewModel)
    }
    else
    {
        @Html.Partial("_FollowPartial", Model.UserInfoViewModel)
    }
}
